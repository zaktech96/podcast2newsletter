</style> <script>
  const faqs = document.querySelectorAll('.faq');

  faqs.forEach((faq, index) => {
    const button = document.getElementById(`faq-${index}`);
    const answer = document.getElementById(`faq-${index}-answer`);

    button.addEventListener('click', () => {
      const isOpen = answer.getAttribute('data-open') === 'true';

      // Toggle clicked FAQ
      answer.setAttribute('data-open', !isOpen);
      button.setAttribute('data-open', !isOpen);
    });
  });
</script> <script>
  const avatar = document.getElementById('avatar');
  const avatarOnline = document.getElementById('avatar-online');
  const messages = document.getElementById('messages');

  function isAvatarAtBottomOfViewport() {
    const avatarRect = avatar.getBoundingClientRect();
    const messagesRect = messages.getBoundingClientRect();

    // Check if avatar has reached the bottom of messages container
    const avatarBottom = avatarRect.top + avatarRect.height;
    const messagesBottom = messagesRect.top + messagesRect.height;

    return avatarBottom >= messagesBottom - 100;
  }

  function checkAvatarPosition() {
    if (isAvatarAtBottomOfViewport()) {
      avatarOnline.setAttribute('data-online', 'false');
    } else {
      avatarOnline.setAttribute('data-online', 'true');
    }
  }

  // Function to handle scroll events
  function handleScroll() {
    checkAvatarPosition();
  }

  // Add scroll event listener to the window
  window.addEventListener('scroll', handleScroll);

  // Check position on page load and window resize
  window.addEventListener('load', checkAvatarPosition);
  window.addEventListener('resize', checkAvatarPosition);
</script>